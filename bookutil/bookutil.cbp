				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-pedantic" />
					<Add option="-Wextra" />
					<Add option="-Wall" />
					<Add option="-DBUILD_DLL" />
					<Add directory="../../../libs/include" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="user32" />
					<Add library="iconv" />
					<Add library="utf8proc" />
					<Add directory="../../../libs" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-DUNICODE" />
			<Add option="-D_UNICODE" />
